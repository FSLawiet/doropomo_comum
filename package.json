{
  "name": "doropomo_comum",
  "version": "0.0.1",
  "description": "Um temporizador doropomo para anti-produtividade",
  "productName": "Doropomo Comum",
  "author": "Fausto Andr√© <fslawliet@gmail.com>",
  "private": true,
  "license": "CC-BY-SA-4.0",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev --modern",
    "dev:ssr": "quasar dev -m ssr --modern",
    "dev:pwa": "quasar dev -m pwa --modern",
    "build": "quasar build --modern",
    "build:pwa": "quasar build -m pwa --modern",
    "version": "auto-changelog -p -l false -u --breaking-pattern 'BREAKING CHANGE:' --release-summary && git add CHANGELOG.md",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@quasar/vite-plugin": "^1.3.0",
    "@supabase/supabase-js": "^2.10.0",
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "quasar": "^2.11.8",
    "uuid": "^9.0.0",
    "v-money": "^0.8.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue-unicode-emoji": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@histoire/plugin-vue": "^0.15.8",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^2.1.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.0",
    "@types/node": "^12.20.21",
    "@types/node-notifier": "^8.0.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^0.15.0",
    "@vue/test-utils": "^2.0.0",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.3.0",
    "cypress": "^12.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "histoire": "^0.15.9",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.15.0",
    "vue-loader": "^16.8.3",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
